import java.util.*;
public class Array_diaongal
{
    int i,j,r,c;
    int[][] arr;
    void input()
    {
      Scanner in = new Scanner(System.in);
      System.out.println("Enter number of rows:");
      r=in.nextInt();
      System.out.println("Enter number of column");
      c=in.nextInt();
      arr=new int[r][c];
      for(i=0;i<r;i++)
      {
        for(j=0;j<c;j++)
        {
         System.out.print("Enter the element at ["+i+"] row and ["+j+"] column");
        }
      } 
    }
}